<script type="text/javascript">
//<![CDATA[
$(function() {
	// Megamenu
	$('.megamenu').megamenu({
		mm_timeout: 50,
		show_method: "simple",
		hide_method: "simple"
	});

	// Account„Å´hover„Åô„Çã„Å®Hello, name„ÇÇËâ≤„ÅåÂ§â„Çè„Çã„Çà„ÅÜ„Å´
	$('#sign-out').hover(function () {
			$('#myaccount-name-wrap .myaccount-name').css('color', '#f9893c');
		}, function() {
			$('#myaccount-name-wrap .myaccount-name').css('color', '#fff');
	});

	// Cart„Å´hover„Åô„Çã„Å®„Ç´„Éº„Éà„Ç¢„Ç§„Ç≥„É≥„ÇÇËâ≤„ÅåÂ§â„Çè„Çã„Çà„ÅÜ„Å´
	$('.header-count-link').hover(function () {
		$('#header-count').children('img.cart-icon-img').attr('src', '/assets/images/header_gloval_nav_icon/icon-btn-cart-orange.png');
		}, function() {
			$('#header-count').children('img.cart-icon-img').attr('src', '/assets/images/header_gloval_nav_icon/icon-btn-cart.png');
	});

	// Submit when Enter key is pressed
	$('.list-form').keypress(function(e) {
		if (e.which == 13) {
			e.preventDefault();
			doSearch();
		}
	});

	$('#schFm').keypress(function(e) {
		if (e.which == 13) {
			e.preventDefault();
			quickTopSearch();
		}
	});

	//aTooltip
	$("#inq-trigger").click(function() {
		$('ul.tabs li.iq a').trigger('click');
	});

	$("a.fixedTip,a.longTip,a.priceTip").on('click', function(e) {
		e.preventDefault();
	});

	$('a.normalTip').aToolTip({
		onShow : function() {
			$('#aToolTip').bgiframe();
		},
		xOffset : -130, // x position
		yOffset : 10, // y position
		fixed : true,
		clickIt : false
	});
	$('a.longTip').aToolTip({
		onShow : function() {
			$('#aToolTip').bgiframe();
		},
		xOffset : -320, // x position
		yOffset : 10, // y position
		fixed : true,
		clickIt : false
	});

	$('a.fixedTip').aToolTip({
		onShow : function() {
			$('#aToolTip').bgiframe();
		},
		xOffset : -20, // x position
		fixed : true,
		clickIt : false
	});

	/**
	 * Right_side SEARCH FOR CARSÂÜÖ
	 */
	$('a.fixedTip.promotion34-fixedTip').aToolTip({
		onShow : function() {
			$('#aToolTip').bgiframe();
		},
		xOffset : -270, // x position
		fixed : true,
		clickIt : false
	});
	$('a.fixedTip.promotion24-fixedTip').aToolTip({
		onShow : function() {
			$('#aToolTip').bgiframe();
		},
		xOffset : -270, // x position
		fixed : true,
		clickIt : false
	});

	$('a.priceTip').aToolTip({
		xOffset : -30, // x position
		yOffset : 0, // y position
		fixed : true,
		clickIt : false
	});

	//Language swicher
	$('#lang-switch-area').hide();

	$('#lang-switch').on({
		mouseover:function(){
			$('#lang-switch-area').show();
		},
		mouseout:function(){
			$('#lang-switch-area').hide();
		}
	});

	$('#lang-switch-area').on({
		mouseout:function(){
			$('#lang-switch-area').hide();
		}
	});

	var url = $('form[name=signin_form]').attr('action');
	var authkey = cookie_check();
	if(authkey != ''){
		change_sign_info('in');
		// „Çµ„Éù„Éº„Çø„Éº„Ç∫ÈÉ®ÂàÜ„ÅÆ„É°„Éã„É•„ÉºÂà∂Âæ°
		isSupporters = cookie_supporters_check();
		change_bfs_menu(isSupporters);

		// Deposit„É™„É≥„ÇØÈùûË°®Á§∫ÂõΩ„ÇíÂèñÂæó
		is_not_show_deposit_link = cookie_deposit_check();
		not_deposit_link(is_not_show_deposit_link);
	}else{
		change_sign_info('out');
	}

	function change_sign_info(sign_status){
		if(sign_status == 'in'){
			$('#sign-out').show();
			$('#point-view').show();
			$('#myaccount-name-wrap').show();
			$('#header-count').show();
			$('#mypage-buynow-cars-box').show();
		}else if(sign_status == 'out'){
			$('#sign-in').show();
			$('#point-view').hide();
			$('#myaccount-name-wrap').hide();
			$('#buynow-button-wrap').hide();

			//202010B017_FrontNoLoginFavorite „Éò„ÉÉ„ÉÄ„ÅÆ„ÅäÊ∞ó„Å´ÂÖ•„Çä„ÅØÂ∏∏ÊôÇË°®Á§∫
			// $('#header-count').hide();
			$('#header-count').show();
            $('#fn-header-middle-buynow').hide();

			$('#mypage-buynow-cars-box').hide();
		}
	}

	function change_bfs_menu(register_status){
		if(register_status == true){
			$('#non_supporters_menu').hide();
			$('#supporters_menu').show();
		}else if(register_status == false){
			$('#non_supporters_menu').show();
			$('#supporters_menu').hide();
		}
		return;
	}

	function not_deposit_link(deposit_status){
		if(deposit_status == true){
			$('#deposit_link').hide();
		}else if(deposit_status == false){
			$('#deposit_link').show();
		}
		return;
	}

	// ‚ñº „Ç´„Ç¶„É≥„Éà„ÉÄ„Ç¶„É≥„Çø„Ç§„Éû„Éº„ÅÆË®≠ÂÆö
	function countDownTimer() {
		var deadline = new Date("January 4,2022, 23:59:59 +0900");
		var deadlineTime = deadline.getTime();
		var deadlineOffset = deadline.getTimezoneOffset() * 60000;
		var deadlineUtc = deadlineTime + deadlineOffset;

		var now = new Date();
		var nowTime = now.getTime();
		var nowOffset = now.getTimezoneOffset() * 60000;
		var nowUtc = nowTime + nowOffset;

		var leftTime = deadlineUtc - nowUtc;

		var leftDays = 0;
		var leftHours = 0;
		var leftMins = 0;
		var leftSecs = 0;
		if (leftTime > 0) {
			// ÊÆã„Çä„ÅÆÊôÇÈñì„ÇíË®àÁÆó
			leftDays = Math.floor(leftTime/(24*60*60*1000));
			leftHours = Math.floor((leftTime % (24*60*60*1000))/(60*60*1000));
			leftMins = Math.floor((leftTime % (24*60*60*1000))/(60*1000))%60;
			leftSecs = Math.floor((leftTime % (24*60*60*1000))/1000)%60%60;
		}else{
			clearInterval(timer);
		}

		$(".count-days").text(( '0' + leftDays ).slice(-2));
		$(".count-hours").text(( '0' + leftHours ).slice(-2));
		$(".count-minutes").text(( '0' + leftMins ).slice(-2));
		$(".count-seconds").text(( '0' + leftSecs ).slice(-2));
	}
	var timer = setInterval(countDownTimer, 1000);

	// Ë®ÄË™ûÂ§âÊõ¥Âá¶ÁêÜ(„Éò„ÉÉ„ÉÄ„Éº„É°„Éã„É•„Éº„Åß„ÅÆË®ÄË™ûÂ§âÊõ¥)
    $('#lang-switch-menu li').click(function(){
		// ÈÅ∏Êäû„Åó„ÅüË®ÄË™û„ÇíÂèñÂæó
		var lang = $(this).data('lang');
		set_language_cookie(lang);
    });

});
//]]>
