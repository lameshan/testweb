$(function(){

	var activate_flag = false;
	var header_search_keyword_action = function(e){
		e.stopPropagation();
		e.preventDefault();

		if (activate_flag) {
			console.error('double click');
			return false;
		}

		// Ê§úÁ¥¢ÂÆüË°åForm„ÅÆÈÅ∏ÂÆö
		var form_id = 'tmpFm';
		if($('#listFm').length > 0){
			// StockListÁîªÈù¢„ÅÆÂ†¥Âêà
			form_id = 'listFm';
		}else if($('#schFm').length > 0){
			// TopÁîªÈù¢„ÅÆÂ†¥Âêà
			form_id = 'schFm';
		}
		var form_selector = '#' + form_id;

		if (form_selector === '#listFm') removeValuesOnExit();

		var $inputs = $('.header-search-keyword-area').find('input[type=text]');
		if ($inputs.length > 0) {
			var keys = [];
			$inputs.each(function(e){
				var $e = $(this);
				$('<input>').attr({type: 'hidden',name: $e.attr('name'),value: $e.val()}).appendTo(form_selector);
			});
			// Ê§úÁ¥¢„É¢„Éº„Éâ
			var kmode_value = $('select[name=kmode]').val();
			if (!kmode_value) {
				kmode_value = 'and';
			}
			$('<input>').attr({type: 'hidden',name: 'kmode',value: kmode_value}).appendTo(form_selector);

			//„Ç™„Éó„Ç∑„Éß„É≥„ÅÆË°®Á§∫
			var showmore = e.data;
			if (showmore === 'top_advanced_search') {
				$('input[name=showmore]').val(1);
			}
		}
		activate_flag = true;
		return searchFormAction(document.getElementById(form_id));
	};

	// „Éò„ÉÉ„ÉÄ„ÉºÈÉ®ÂàÜ„ÅÆÊ§úÁ¥¢„Éú„Çø„É≥„ÇØ„É™„ÉÉ„ÇØ
	$('#header-anchor-of-search-button').click(function(e){
		searchBarOnly();
		return header_search_keyword_action(e);
	});
	// „Éò„ÉÉ„ÉÄ„ÉºÈÉ®ÂàÜ„ÅÆÊ§úÁ¥¢„Éï„Ç©„Éº„É†ÂÜÖ„ÅßEnter„Ç≠„Éº„ÇØ„É™„ÉÉ„ÇØ
	$('#header-search-keyword').keypress(function(e){
		if(e.which == 13) {
			searchBarOnly();
			return header_search_keyword_action(e);
		}
	});

	// „Éò„ÉÉ„ÉÄ„ÉºÈÉ®ÂàÜ‰ª•Â§ñ„ÅÆ„É™„É≥„ÇØÁ≠â„Åã„ÇâÊ§úÁ¥¢ÂÆüË°å„Åô„Çã„Åü„ÇÅ„ÅÆÂÖ•Âè£
	$('#dummy-anchor-of-search-button').click(function(e){
		return header_search_keyword_action(e);
	});

	// Advanced Search„ÇØ„É™„ÉÉ„ÇØ
	$('#top-advanced-search').click('top_advanced_search', function(e){
		return header_search_keyword_action(e);
	});

	//Search Bar„ÅÆÂÖ•ÂäõÈ†ÖÁõÆ„ÅÆ„Åø„ÅßÊ§úÁ¥¢
	function searchBarOnly(){
		// StocklistÁîªÈù¢„ÅÆÂ†¥Âêà„ÅÆ„ÅøÈÅ©Áî®
		if ($('#listFm').length !== 1) {
			return;
		}

		// „Éï„É™„Éº„ÉØ„Éº„Éâ„ÅÆÂÖ•Âäõ„Åå„ÅÇ„ÇãÂ†¥Âêà„ÅÆ„ÅøÈÅ©Áî®
		var keyword = $('#header-search-keyword').val();
		if (keyword === '') {
			return;
		}

		// Ê§úÁ¥¢„Éú„ÉÉ„ÇØ„Çπ„É™„Çª„ÉÉ„Éà
		runReset();

	}

	// „Ç≠„Éº„ÉØ„Éº„ÉâÊ§úÁ¥¢„Éï„Ç©„Éº„É†„ÅÆÊ®™ÂπÖ„ÇíË™øÊï¥„Åô„Çã
	function fixSearchKeywordWidth(){
		var w = $('#in-header-middle-wrap').outerWidth(true) - $('#header-count').outerWidth(true) - $('.header-account-info-wrap').outerWidth(true) - $('.search-keyword-menu').outerWidth(true) - $('#header-search-button').outerWidth(true);
		w = w - 50; // ‰ΩôÁôΩÂàÜ
		$('#header-search-keyword').width(w);
	}

	$(window).resize(function(){
		fixSearchKeywordWidth();
	});
	fixSearchKeywordWidth();

});

</script>
